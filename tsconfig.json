{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": ".",
    "strict": true,
    "paths": {
      "tailwind.config": [
        "tailwind.config.js"
      ],
      "@/*": [
        "./src/*"
      ],
      "@animations/*": [
        "./src/animations/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@navigation/*": [
        "./src/navigation/*"
      ],
      "@screens/*": [
        "./src/app/*"
      ],
      "@store/*": [
        "./src/store/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}